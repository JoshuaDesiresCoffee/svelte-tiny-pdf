{
    "name": "svelte-tiny-pdf",
    "version": "1.0.1",
    "description": "A tiny PDF viewer for Svelte",
    "homepage": "https://github.com/JoshuaDesiresCoffee/svelte-tiny-pdf",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JoshuaDesiresCoffee/svelte-tiny-pdf.git"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "svelte": "./dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "svelte": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "svelte-package",
        "dev": "svelte-package -w",
        "prepublishOnly": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "check": "svelte-check"
    },
    "keywords": [
        "svelte",
        "pdf",
        "pdf-viewer",
        "pdfjs",
        "pdfjs-dist"
    ],
    "author": "JoshuaDesiresCoffee",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "peerDependencies": {
        "svelte": "^3.55.0 || ^4.0.0"
    },
    "dependencies": {
        "pdfjs-dist": "^4.7.76"
    },
    "devDependencies": {
        "@sveltejs/package": "^2.3.7",
        "svelte": "^4.2.19",
        "svelte-check": "^4.0.9"
    }
}
